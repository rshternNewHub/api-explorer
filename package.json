{
  "name": "api-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run start:host\" \"npm run start:request\" \"npm run start:response\" \"npm run start:analytics\" \"npm run start:collection\"",
    "start:host": "cd host && npm start",
    "start:request": "cd request-builder && npm start",
    "start:response": "cd response-viewer && npm start",
    "start:analytics": "cd analytics && npm start",
    "start:collection": "cd collection-manager && npm start",
    "install:all": "concurrently \"cd host && npm install\" \"cd request-builder && npm install\" \"cd response-viewer && npm install\" \"cd analytics && npm install\" \"cd collection-manager && npm install\"",
    "build": "tsc -b && vite build",
    "lint": "npm run lint:host && npm run lint:requestBuilder && npm run lint:responseViewer && npm run lint:collectionManager && npm run lint:analytics",
    "lint:host": "cd apps/host && npm run lint",
    "lint:request-builder": "cd apps/requestBuilder && npm run lint",
    "format": "prettier --write \"apps/**/*.{ts,tsx,css,md}\"",
    "type-check:host": "cd apps/host && tsc --noEmit",
    "type-check:requestBuilder": "cd request-builder && tsc --noEmit",
    "type-check:responseViewer": "cd response-viewer && tsc --noEmit",
    "type-check:collectionManager": "cd collection-manager && tsc --noEmit",
    "type-check:analytics": "cd analytics && tsc --noEmit",
    "type-check": "npm run type-check:host && npm run type-check:requestBuilder && npm run type-check:responseViewer && npm run type-check:collectionManager && npm run type-check:analytics",
    "validate": "npm run lint && npm run format && npm run type-check",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "react-router-dom": "^7.1.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
