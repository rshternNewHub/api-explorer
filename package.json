{
  "name": "api-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:host": "npm run dev --prefix apps/host",
    "dev:request-builder": "npm run dev --prefix apps/request-builder",
    "dev:response-viewer": "npm run dev --prefix apps/response-viewer",
    "dev:collection-manager": "npm run dev --prefix apps/collection-manager",
    "dev:analytics": "npm run dev --prefix apps/analytics",
    "dev:all": "concurrently \"npm run dev:host\" \"npm run dev:request-builder\" \"npm run dev:response-viewer\" \"npm run dev:collection-manager\" \"npm run dev:analytics\"",
    "build": "tsc -b && vite build",
    "lint": "npm run lint:host && npm run lint:request-builder && npm run lint:response-viewer && npm run lint:collection-manager && npm run lint:analytics",
    "lint:host": "cd apps/host && npm run lint",
    "lint:request-builder": "cd apps/request-builder && npm run lint",
    "format": "prettier --write \"apps/**/*.{ts,tsx,css,md}\"",
    "type-check:host": "cd apps/host && tsc --noEmit",
    "type-check:request-builder": "cd apps/request-builder && tsc --noEmit",
    "type-check:response-viewer": "cd apps/response-viewer && tsc --noEmit",
    "type-check:collection-manager": "cd apps/collection-manager && tsc --noEmit",
    "type-check:analytics": "cd apps/analytics && tsc --noEmit",
    "type-check": "npm run type-check:host && npm run type-check:request-builder && npm run type-check:response-viewer && npm run type-check:collection-manager && npm run type-check:analytics",
    "validate": "npm run lint && npm run format && npm run type-check",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
